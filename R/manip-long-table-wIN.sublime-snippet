# Convert table to long format for with-in subjects ANOVA
library(reshape2)
${1:data}.long = melt(${1:data}, id = c("${2:participant}","${3:column}","${4:age}"), # keep these columns the same
                  measure = c("${5:level1}","${6:level2}"), 				 # Put these two columns into a new column
                  variable.name="${7:factorName}") 					# Name of the new column with the labels
summary(${1:data}.long)
]]></content>
